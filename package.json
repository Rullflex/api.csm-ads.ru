{
  "name": "api.csm-ads.ru",
  "type": "module",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc && tsc-alias -p tsconfig.json",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "build:prod": "NODE_OPTIONS='--max-old-space-size=4096' tsc && tsc-alias",
    "start:prod": "fastify start -l info dist/app.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/sensible": "^6.0.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "puppeteer": "^24.16.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@types/node": "^22.1.0",
    "@types/qs": "^6.14.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "eslint": "^9.33.0",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.2"
  }
}
